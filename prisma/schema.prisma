generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Artist {
  id         String @id @default(uuid())
  name       String
  genre      String
  followers  Int
  image      String
  popularity Int
}

model Track {
  id      String @id @default(uuid())
  title   String
  image   String
  preview String
}
